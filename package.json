{
  "name": "miru",
  "version": "0.0.0",
  "private": "true",
  "packageManager": "pnpm@10.12.4",
  "type": "module",
  "scripts": {
    "dev": "pnpm run docs:dev",
    "dev:slow": "VITE_DEV_SLOW_DOWN_MS=5000 pnpm run dev",
    "build": "vue-tsc -b tsconfig.check.json && NODE_ENV=production rollup --config rollup.config.js",
    "build:dev": "NODE_ENV=development rollup --config rollup.config.js",
    "check": "vue-tsc -b tsconfig.check.json --incremental",
    "lint:markdown": "markdownlint --ignore-path .gitignore --ignore '**/CHANGELOG.md' '**/*.md'",
    "lint:prettier": "prettier --check '**/*.{ts,js,tsx,jsx,cjs,vue,md,html,yaml,yml,json}'",
    "lint": "stylelint '**/*.css' && pnpm run lint:markdown && pnpm eslint --quiet && pnpm run lint:prettier",
    "lint:fix": "stylelint --fix '**/*.css' && pnpm run lint:markdown --fix && pnpm eslint --quiet --fix && pnpm lint:prettier --write",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "update-deps": "taze minor --recursive --interactive --write --install --update && pnpm update && pnpm dedupe",
    "test": "vitest run"
  },
  "author": "Taye Adeyemi",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "compare-palettes": "workspace:^",
    "filesize": "^10.1.6",
    "reactive-effects": "workspace:^",
    "shared": "workspace:^",
    "uid": "catalog:",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-i18n-lite": "^1.0.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.0",
    "@eslint/markdown": "^6.6.0",
    "@iconify-json/tabler": "^1.2.19",
    "@lokesh.dhakar/quantize": "catalog:",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/color-diff": "^1.2.5",
    "@types/debug": "^4.1.12",
    "@types/dom-webcodecs": "^0.1.15",
    "@types/ebml": "^3.0.5",
    "@types/json-pointer": "^1.0.34",
    "@types/node": "^24.0.7",
    "@types/quantize": "^1.0.2",
    "@types/semver": "^7.7.0",
    "@types/stats.js": "^0.17.4",
    "@types/three": "catalog:",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/preset-icons": "^66.3.2",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "color-diff": "catalog:",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-recommended-bump": "^11.2.0",
    "cssnano": "^7.0.7",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.0",
    "eslint-config-love": "^121.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "fine-jsx": "catalog:",
    "glob": "^11.0.3",
    "glslify": "^7.1.1",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "markdown-it-task-lists": "^2.1.1",
    "markdownlint-cli": "^0.45.0",
    "nano-spawn": "^1.0.2",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-hover-media-feature": "^1.0.2",
    "postcss-import": "^16.1.1",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.2.4",
    "postcss-url": "^10.1.3",
    "prettier": "^3.6.2",
    "rollup": "^4.44.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild-transform": "^1.5.0",
    "rollup-plugin-external-assets": "^4.1.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-glsl-optimize": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semver": "^7.7.2",
    "sharp": "^0.34.3",
    "stats.js": "catalog:",
    "stylelint": "^16.21.0",
    "stylelint-config-css-modules": "^4.5.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "taze": "^19.1.0",
    "terser": "^5.43.1",
    "throttle-debounce": "catalog:",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "vite": "6.1.0",
    "vite-bundle-analyzer": "^0.23.0",
    "vite-plugin-remote-assets": "^2.0.0",
    "vitepress": "2.0.0-alpha.2",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "**/*.((ts|js|cjs)?(x)|vue)": [
      "eslint --quiet --fix",
      "prettier --check --write"
    ],
    "**/*.md": [
      "eslint --quiet --fix",
      "markdownlint --fix",
      "prettier --check --write"
    ],
    "**/*.css": "stylelint --fix",
    "**/*.(yaml|yml|json)": "prettier --check --write"
  },
  "browserslist": [
    "defaults and fully supports es6-module and fully supports webgl2",
    "maintained node versions"
  ],
  "pnpm": {
    "patchedDependencies": {
      "glsl-tokenizer": "patches/glsl-tokenizer.patch",
      "libavjs-webcodecs-polyfill": "patches/libavjs-webcodecs-polyfill.patch",
      "videocontext": "patches/videocontext.patch",
      "@mediapipe/tasks-vision": "patches/@mediapipe__tasks-vision.patch"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "rollup-plugin-glsl-optimize"
    ]
  }
}
