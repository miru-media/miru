{
  "name": "miru",
  "version": "0.0.0",
  "packageManager": "pnpm@9.10.0",
  "type": "module",
  "main": "dist/miru.umd.js",
  "style": "dist/miru.css",
  "exports": {
    ".": {
      "import": "./dist/miru.js",
      "require": "./dist/miru.umd.js"
    },
    "./vue2": {
      "import": "./dist/vue2.js",
      "require": "./dist/vue2.umd.js"
    },
    "./miru.css": "./dist/miru.css"
  },
  "scripts": {
    "dev": "vite",
    "dev:slow": "VITE_DEV_SLOW_DOWN_MS=5000 pnpm run dev",
    "build": "pnpm check && NODE_ENV=production rollup --config rollup.config.js",
    "build:dev": "NODE_ENV=development rollup --config rollup.config.js",
    "check": "tsc --incremental",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky"
  },
  "author": "Taye Adeyemi",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@reactively/core": "^0.0.8",
    "cropperjs": "^1.6.2",
    "gl-matrix": "^3.4.3",
    "glsl-blend": "^1.0.3",
    "glsl-film-grain": "^1.0.4",
    "glsl-luma": "^1.0.1",
    "glsl-noise": "^0.0.0",
    "glsl-vignette": "^1.1.0",
    "throttle-debounce": "^5.0.2",
    "twgl.js": "^5.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@iconify-json/solar": "^1.2.0",
    "@iconify-json/tabler": "^1.2.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.0",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/preset-icons": "^0.62.3",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "cssnano": "^7.0.6",
    "esbuild": "^0.23.1",
    "eslint": "^9.11.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "glslify": "^7.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.0",
    "postcss": "^8.4.47",
    "postcss-hover-media-feature": "^1.0.2",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.0.5",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.3",
    "rollup": "^4.22.5",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-esbuild-transform": "^1.5.0",
    "rollup-plugin-external-assets": "^4.1.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-glsl-optimize": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.31.6",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.5.0",
    "unocss": "^0.62.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "vite": "5.4.5",
    "vite-bundle-analyzer": "^0.10.6"
  },
  "lint-staged": {
    "**/*.((ts|js|cjs)?(x)|vue|md)": [
      "eslint --fix",
      "prettier --check --write"
    ],
    "**/*.(yaml|yml|json|css)": "prettier --check --write",
    "**/*.css": "stylelint --fix"
  },
  "browserslist": [
    "defaults and fully supports es6-module and fully supports webgl2",
    "maintained node versions"
  ],
  "pnpm": {
    "patchedDependencies": {
      "glsl-tokenizer": "patches/glsl-tokenizer.patch"
    }
  }
}
