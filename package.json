{
  "name": "miru",
  "version": "0.0.0",
  "packageManager": "pnpm@9.10.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:slow": "VITE_DEV_SLOW_DOWN_MS=5000 pnpm run dev",
    "build": "pnpm check && NODE_ENV=production rollup --config rollup.config.js",
    "build:dev": "NODE_ENV=development rollup --config rollup.config.js",
    "check": "tsc --incremental",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky"
  },
  "author": "Taye Adeyemi",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@iconify-json/tabler": "^1.2.5",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.7",
    "@types/stats.js": "^0.17.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/preset-icons": "^0.63.4",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "filesize": "^10.1.6",
    "glslify": "^7.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.1",
    "postcss": "^8.4.47",
    "postcss-hover-media-feature": "^1.0.2",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.0.7",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-esbuild-transform": "^1.5.0",
    "rollup-plugin-external-assets": "^4.1.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-glsl-optimize": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.2",
    "stats.js": "catalog:",
    "stylelint": "^16.10.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.36.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "unocss": "^0.63.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "vite": "5.4.9",
    "vite-bundle-analyzer": "^0.12.1",
    "vite-plugin-remote-assets": "^0.5.0"
  },
  "lint-staged": {
    "**/*.((ts|js|cjs)?(x)|vue|md)": [
      "eslint --quiet --fix",
      "prettier --check --write"
    ],
    "**/*.(yaml|yml|json|css)": "prettier --check --write",
    "**/*.css": "stylelint --fix"
  },
  "browserslist": [
    "defaults and fully supports es6-module and fully supports webgl2",
    "maintained node versions"
  ],
  "pnpm": {
    "patchedDependencies": {
      "glsl-tokenizer": "patches/glsl-tokenizer.patch"
    }
  }
}
